<%- include('../partials/header.ejs') %> 

  <h2>Browse All of Our Photos</h2>

  	<ul>
  		
  		<% photos.forEach ((photo) => { %>
  			<li>
  				<a href="/photos/<%= photo.id %>"><%= photo.title %></a>
  			</li>
  			<img src="<%= photo.url %>">
  		<%})%>

  	</ul>




